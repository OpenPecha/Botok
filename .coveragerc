[run]
source = botok
omit = */tests/*,*/test_*,setup.py,botok/utils/expose_data.py,botok/utils/lenient_normalization.py,botok/utils/unicode_normalization.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
fail_under = 80
show_missing = True
